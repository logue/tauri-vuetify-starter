{
  "name": "@tauri-vuetify-starter/docs",
  "description": "Documentation site for Tauri Vue3 application template",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "clean": "rimraf .nuxt .output public/_nuxt",
    "clean:full": "rimraf .nuxt .output public/_nuxt node_modules/.vite node_modules/.cache",
    "clean:reinstall": "pnpm run clean:full && pnpm install",
    "deploy": "nuxt generate && gh-pages -d .output/public -b main",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier src/**/*.{ts,vue} -w -u",
    "lint:style": "stylelint \"./src/**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint && prettier \"./src/**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "nuxt:prepare": "nuxt prepare",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@nuxt/content": "^3.11.2",
    "@nuxt/eslint": "^1.15.1",
    "@nuxtjs/i18n": "^10.2.3",
    "@nuxtjs/sitemap": "^7.6.0",
    "@pinia/nuxt": "^0.11.3",
    "better-sqlite3": "^12.6.2",
    "nuxt": "^4.3.1",
    "nuxt-gtag": "^4.1.0",
    "pinia": "^3.0.4",
    "ufo": "^1.6.3",
    "vite-plugin-vuetify": "^2.1.3",
    "vue": "^3.5.28",
    "vue-router": "^5.0.2",
    "vuetify": "^3.11.8",
    "vuetify-nuxt-module": "^0.19.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^10.0.0",
    "eslint-plugin-vuejs-accessibility": "^2.5.0",
    "rimraf": "^6.1.3",
    "sass-embedded": "^1.97.3"
  }
}